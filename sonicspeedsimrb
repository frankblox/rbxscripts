local library = loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/0x"))()

local w1 = library:Window("Sonic Speed Sim") -- Text

game.StarterGui:SetCore("SendNotification", {
    Title = "Credits: FrankBlox Scripts",
    Text = "discord.io/FrankBlox",
    Duration = 500,
    })

    w1:Button(
        "Collect All & Finish Obby",
        function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/78n/Amity/main/RBBattlesSonicSpeedSimulator.lua"))()
        end
    ) -- Text, Callback


w1:Button(
    "Destroy GUI",
    function()
        for i, v in pairs(game.CoreGui:GetChildren()) do
            if v:FindFirstChild("Top") then
                v:Destroy()
            end
        end
    end
) -- Text, Callback

w1:Label("YT: FrankBlox Scripts") -- Text
